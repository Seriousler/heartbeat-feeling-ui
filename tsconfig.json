{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@/*":["./src/*"],
      "@/utils/*":["./src/utils/*"],
      "@/assets/*":["./src/assets/*"],
      "@/api/*":["./src/api/*"],
      "@/pages/*":["./src/pages/*"],
      "@/store/*":["./src/store/*"],
      "@/components/*":["./src/components/*"],
      "@/project/*":["./src/project/*"]
    },
  },
  "include": ["./src", "./types", "./scripts"],
  "compileOnSave": false
}
